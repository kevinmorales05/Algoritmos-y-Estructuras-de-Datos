#include <iostream>
using namespace std;

void agregar(int a[], int n);
void imprimir(int a[], int n);
void menor(int a[], int n);
void ordenar(int a[], int n);

int main() {
 //Con esto ingreso valores para mi arreglo
 int n;
 cout<<"Escriba el tamanio deseado del arreglo"<<endl;
 cin>>n;
 int b[n];
 
 agregar(b, n);
 ordenar(b,n);
 imprimir(b,n);
 
 return 0;

}




void agregar(int a[], int n)
{
  for (int i=0; i<n; i++) 
{
   cin>> a[i];
}

}

void imprimir(int a[], int n)
{
    for (int i=1; i<n; i++)
    {
      cout<<a[i];
    }
}


void menor(int a[], int n) 
{
     int b;
     b=a[0];
     for (int i=1; i<n; i++) 
      {
        if (b>a[i]) 
        {
          b= a[i];
        }
      }
      cout<< "El menor de los nÃºmeros es: "<<b;

}

void ordenar(int a[], int n){

int aux, min;

for (int i=0; i<5; i++)
{
  min=i;
  for (int j=i+1; j<5;j++)
  {
    if (a[j]<a[min])
    {
      min=j;
    }
  }
  aux=a[i];
  a[i]= a[min];
  a[min]=aux;
}

for (int i=0; i<5; i++)
    {
      cout<<a[i];
    }

}
