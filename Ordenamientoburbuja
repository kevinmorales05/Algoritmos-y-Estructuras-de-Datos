#include <iostream>

using namespace std;
//PROTOTIPOS
void ingresar(int a[], int n);
//voy a ingresar valores, tiene un arreglo y tiene un tamano n.
void burbujar(int a[], int n);
//se va a trabajar con el arreglo
void imprimir(int a[], int n);

int main()
{

 int arreglo1[5];
 ingresar( arreglo1, 5);
 burbujar( arreglo1, 5);
imprimir(arreglo1, 5);



return 0;
}
// 2. Trabajo con las funciones
void ingresar(int a[], int n)
{
    //con esto ingreso valores a un arreglo
    for (int i; i<n; i++)
    {
        cout<< "elemento "<<i+1<<endl;
        3..10
        cin>> a[i];
    }
}

//voy a ingresar valores, tiene un arreglo y tiene un tamano n.
void burbujar(int a[], int n)
//se va a trabajar con el arreglo
{

for (int i=0; i<n; i++)
    {
            for (int j=i+1; j<=n-1 ;j++ )
                {
                if (a[i]>a[j])
                    {

                    int aux= a[i];
                    a[i] = a[j];
                    a[j] = aux;

                    }
                }
    }
}
//copio el codigo anterior
void imprimir(int a[], int n){
for ( int i=0; i<n; i++)
  {
  cout<<a[i]<<" ";
  }
}
